<!DOCTYPE html>
<html>
    <head>
        <title>IT Assets Inventory</title>
        <link rel="stylesheet" href="/css/styles.css" />
        <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    </head>
    <body>
        <h2><i>l33t h@x0r5</i> Asset Inventory</h2>
        
        <a href="/" id="cancel-asset">Back</a>
        <table>
            <tr>
                <th>ID</th>
                <th>Category</th>
                <th>Manufacturer</th>
                <th>Model</th>
                <th>Serial Number</th>
                <th>Acquired Date</th>
                <th>Purchase Price</th>
                <th>Condition</th>
                <th>Warrenty Expiration</th>
                <th>Located</th>
                <th>Retired</th>
                <th>Description</th>
                <th>Comments</th>
            </tr>
            {{#each item}}
            <tr>
                    <td>{{this.id}}</td>
                    <td>{{this.category}}</td>
                    <td>{{this.manufacturer}}</td>
                    <td>{{this.model}}</td>
                    <td>{{this.serial}}</td>
                    <td>{{this.acquired}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.cond}}</td>
                    <td>{{this.warrenty}}</td>
                    <td>{{this.location}}</td>
                    <td>{{this.retired}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.comments}}</td>
                </tr>
            {{/each}}
        </table>
        
